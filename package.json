{
  "name": "pocket-mirror",
  "version": "1.2.6",
  "main": "expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "android:dev:client": "expo start --android --dev-client",
    "ios:dev:client": "expo start --ios --dev-client",
    "eas:build:development:android:emulator+device": "eas build --profile development --platform android",
    "eas:build:development:ios:simulator": "eas build --profile development-simulator --platform ios",
    "eas:build:development:ios:device": "eas build --profile development --platform ios",
    "eas:build:preview:android": "eas build --profile preview --platform android",
    "eas:build:preview:ios:simulator": "eas build --profile preview-simulator --platform ios",
    "eas:build:preview:ios:device": "eas build --profile preview --platform ios",
    "eas:build:production:android": "eas build --profile production --platform android",
    "eas:build:production:ios": "eas build --profile production --platform ios",
    "eas:build:run:android": "eas build:run -p android",
    "eas:build:run:ios": "eas build:run -p ios",
    "eas:submit:internal:android": "eas submit --profile internal --platform android",
    "eas:submit:alpha:android": "eas submit --profile alpha --platform android",
    "eas:submit:beta:android": "eas submit --profile beta --platform android",
    "eas:submit:production:android": "eas submit --profile production --platform android",
    "eas:submit:production:ios": "eas submit --profile production --platform ios",
    "test": "jest",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --check .",
    "format:fix": "prettier --write ."
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|i18n-js|react-redux)"
    ],
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js",
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!metro.config.js",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/store/index.js",
      "!**/store/StoreTestUtils.js",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/typedefs.js"
    ]
  },
  "dependencies": {
    "@expo/metro-config": "~0.18.8",
    "@mmomtchev/expo-android-localized-app-name": "^1.0.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/slider": "4.5.2",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@reduxjs/toolkit": "^2.2.6",
    "expo": "~51.0.22",
    "expo-brightness": "~12.0.1",
    "expo-camera": "~15.0.13",
    "expo-constants": "~16.0.2",
    "expo-dev-client": "~4.0.20",
    "expo-localization": "~15.0.3",
    "expo-status-bar": "~1.12.1",
    "expo-updates": "~0.25.18",
    "i18n-js": "^4.4.3",
    "lottie-react-native": "6.7.0",
    "react": "18.2.0",
    "react-native": "0.74.3",
    "react-native-gesture-handler": "~2.16.1",
    "react-native-reanimated": "~3.10.1",
    "react-native-safe-area-context": "4.10.1",
    "react-native-screens": "3.31.1",
    "react-native-webview": "13.8.6",
    "react-redux": "^9.1.2",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.5.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.79",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^13.0.0",
    "eslint-plugin-jest": "^28.5.0",
    "eslint-plugin-react-native": "^4.1.0",
    "jest": "^29.7.0",
    "jest-expo": "~51.0.3",
    "prettier": "^3.2.5",
    "simple-git-hooks": "^2.11.1"
  },
  "private": true
}
