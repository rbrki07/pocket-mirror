{
  "name": "pocket-mirror",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "android:dev:client": "expo start --android --dev-client",
    "ios:dev:client": "expo start --ios --dev-client",
    "eas:build:development:android:emulator+device": "eas build --profile development --platform android",
    "eas:build:development:ios:simulator": "eas build --profile development-simulator --platform ios",
    "eas:build:development:ios:device": "eas build --profile development --platform ios",
    "eas:build:preview:android": "eas build --profile preview --platform android",
    "eas:build:preview:ios:simulator": "eas build --profile preview-simulator --platform ios",
    "eas:build:preview:ios:device": "eas build --profile preview --platform ios",
    "eas:build:production:android": "eas build --profile production --platform android",
    "eas:build:production:ios": "eas build --profile production --platform ios",
    "eas:build:run:android": "eas build:run -p android",
    "eas:build:run:ios": "eas build:run -p ios",
    "eas:submit:internal:android": "eas submit --profile internal --platform android",
    "eas:submit:alpha:android": "eas submit --profile alpha --platform android",
    "eas:submit:beta:android": "eas submit --profile beta --platform android",
    "eas:submit:production:android": "eas submit --profile production --platform android",
    "eas:submit:production:ios": "eas submit --profile production --platform ios",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|i18n-js)"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/store/index.js",
      "!**/store/StoreTestUtils.js",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/typedefs.js"
    ]
  },
  "dependencies": {
    "@expo/metro-config": "^0.10.7",
    "@mmomtchev/expo-android-localized-app-name": "^1.0.1",
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "@reduxjs/toolkit": "^1.9.5",
    "expo": "^49.0.8",
    "expo-brightness": "~11.4.0",
    "expo-camera": "~13.4.2",
    "expo-constants": "~14.4.2",
    "expo-dev-client": "~2.4.8",
    "expo-localization": "~14.3.0",
    "expo-status-bar": "~1.6.0",
    "expo-updates": "~0.18.12",
    "i18n-js": "^4.3.0",
    "lottie-react-native": "5.1.6",
    "react": "18.2.0",
    "react-native": "0.72.4",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-webview": "13.2.2",
    "react-redux": "^8.1.2",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.2.2",
    "@types/jest": "^29.5.4",
    "eslint": "^8.47.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.6.4",
    "jest-expo": "^49.0.0",
    "prettier": "^3.0.2"
  },
  "overrides": {
    "@expo/config-plugins": "~7.2.2"
  },
  "private": true
}
