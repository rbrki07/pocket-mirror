{
  "name": "pocket-mirror",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "android:dev:client": "expo start --android --dev-client",
    "ios:dev:client": "expo start --ios --dev-client",
    "eas:build:development:android:emulator+device": "eas build --profile development --platform android",
    "eas:build:development:ios:simulator": "eas build --profile development-simulator --platform ios",
    "eas:build:development:ios:device": "eas build --profile development --platform ios",
    "eas:build:preview:android": "eas build --profile preview --platform android",
    "eas:build:preview:ios:simulator": "eas build --profile preview-simulator --platform ios",
    "eas:build:preview:ios:device": "eas build --profile preview --platform ios",
    "eas:build:run:android": "eas build:run -p android",
    "eas:build:run:ios": "eas build:run -p ios",
    "test": "jest"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|i18n-js)"
    ],
    "setupFiles": [
      "<rootDir>/jest.setup.js"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/dist/**",
      "!**/node_modules/**",
      "!**/store/index.js",
      "!**/store/StoreTestUtils.js",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!**/typedefs.js"
    ]
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.17.11",
    "@react-native-community/slider": "4.4.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "expo": "~48.0.6",
    "expo-brightness": "~11.2.1",
    "expo-camera": "~13.2.1",
    "expo-dev-client": "~2.2.1",
    "expo-localization": "~14.1.1",
    "expo-status-bar": "~1.4.4",
    "expo-updates": "~0.16.4",
    "i18n-js": "^4.2.3",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-redux": "^8.0.5",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "lottie-react-native": "5.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@types/jest": "^29.5.2",
    "eslint": "^8.36.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.0.0",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "prettier": "^2.8.7"
  },
  "private": true
}
