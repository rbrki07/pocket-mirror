{
  "expo": {
    "name": "Pocket Mirror",
    "slug": "pocket-mirror",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "buildNumber": "1.0.0",
      "bundleIdentifier": "de.rene-wilby.pocket-mirror",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_SETTINGS",
        "CAMERA"
      ],
      "blockedPermissions": [
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "VIBRATE",
	      "SYSTEM_ALERT_WINDOW",
        "READ_MEDIA_AUDIO",
        "READ_MEDIA_IMAGES",
        "READ_MEDIA_VIDEO",
        "READ_MEDIA_VISUAL_USER_SELECTED"
      ],
      "package": "de.rene_wilby.pocket_mirror",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "The app wants to be able to access the front camera to use it as a mirror. No image captures are saved."
        }
      ],
      "expo-localization",
      "@mmomtchev/expo-android-localized-app-name"
    ],
    "locales": {
      "da": "./i18n/da.json",
      "de": "./i18n/de.json",
      "en": "./i18n/en.json",
      "es": "./i18n/es.json",
      "fi": "./i18n/fi.json",
      "fr": "./i18n/fr.json",
      "it": "./i18n/it.json",
      "nb": "./i18n/nb.json",
      "nl": "./i18n/nl.json",
      "pt": "./i18n/pt.json",
      "sv": "./i18n/sv.json"
    },
    "extra": {
      "eas": {
        "projectId": "1523d6ea-e739-4e08-a634-998233267c7d"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1523d6ea-e739-4e08-a634-998233267c7d"
    }
  }
}
