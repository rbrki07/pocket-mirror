{
  "expo": {
    "name": "Pocket Mirror",
    "slug": "pocket-mirror",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.audience-engagement.pocket-mirror"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_SETTINGS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "de.audience_engagement.pocket_mirror"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Die App möchte auf die Front-Kamera zugreifen können, um sie als Spiegel zu verwenden. Es werden keine Bildaufnahmen gespeichert."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "1523d6ea-e739-4e08-a634-998233267c7d"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1523d6ea-e739-4e08-a634-998233267c7d"
    }
  }
}
