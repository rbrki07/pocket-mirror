{
  "expo": {
    "name": "Pocket Mirror",
    "slug": "pocket-mirror",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "de.audience-engagement.pocket-mirror",
      "infoPlist": {
        "CFBundleAllowMixedLocalizations": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "WRITE_SETTINGS",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "package": "de.audience_engagement.pocket_mirror"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "The app wants to be able to access the front camera to use it as a mirror. No image captures are saved."
        }
      ],
      "expo-localization"
    ],
    "locales": {
      "da": "./i18n/da.json",
      "de": "./i18n/de.json",
      "en": "./i18n/en.json",
      "fi": "./i18n/fi.json",
      "nb": "./i18n/nb.json",
      "nl": "./i18n/nl.json",
      "sv": "./i18n/sv.json"
    },
    "extra": {
      "eas": {
        "projectId": "1523d6ea-e739-4e08-a634-998233267c7d"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/1523d6ea-e739-4e08-a634-998233267c7d"
    }
  }
}
